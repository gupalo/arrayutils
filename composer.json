{
    "name": "gupalo/arrayutils",
    "description": "Array utils",
    "type": "library",
    "license": "MIT",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": ">=8.4",
        "symfony/property-access": "^7.4|^8.0"
    },
    "require-dev": {
        "phpstan/phpstan": "^2.1",
        "phpunit/phpunit": "^11.5"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "platform": {
            "php": "8.4"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "Gupalo\\ArrayUtils\\": "src/ArrayUtils/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Gupalo\\Tests\\ArrayUtils\\": "tests/ArrayUtils/"
        }
    }
}
